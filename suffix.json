{
  "pt-BR": [
    {
      "pattern": "([a-zA-Z])mn\\b",
      "replace": "\\1mente, enabled"
    },
    {
      "pattern": "([a-zA-Z])ao\\b",
      "replace": "\\1ão, enabled"
    },
    {
      "pattern": "([a-zA-Z])çç\\b",
      "replace": "\\1ção, enabled"
    },
    {
      "pattern": "([a-zA-Z])ii\\b",
      "replace": "\\1inho, enabled"
    },
    {
      "pattern": "^(aa)\\b",
      "replace": "à, enabled"
    }
  ],
  "en": [
    {
      "pattern": "([a-zA-Z])ab\\b",
      "replace": "\\1able, enabled"
    },
    {
      "pattern": "([a-zA-Z])ns\\b",
      "replace": "\\1ness, disabled"
    },
    {
      "pattern": "([a-zA-Z])ls\\b",
      "replace": "\\1less, disabled"
    }
  ],
  "de": [
    {
      "pattern": "([a-zA-Z])be\\b",
      "replace": "\\1beispiel, disabled"
    }
  ],
  "it": [
    {
      "pattern": "([a-zA-Z])zn\\b",
      "replace": "\\1zione, disabled"
    }
  ],
  "fr": [
    {
      "pattern": "([a-zA-Z])ex\\b",
      "replace": "\\1exemple, disabled"
    }
  ],
  "es": [
    {
      "pattern": "([a-zA-Z])ej\\b",
      "replace": "\\1ejemplo, disabled"
    }
  ]
}